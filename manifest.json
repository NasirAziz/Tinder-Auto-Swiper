{
	"manifest_version": 3,
	"name": "Tinder Auto Swiper",
	"version": "1.0",
	"permissions": ["activeTab", "storage", "alarms", "scripting"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"icons": {
		"48": "icon.png"
	}
}
